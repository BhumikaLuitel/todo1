@model List<TodoApp.Models.Todo>
<h4>Index is here</h4>
<table>
    <thead>
        <tr>
            <th> Id</th>
            <th> Employee</th>
            <th> Work</th>
            <th> Description</th>
            <th> Duration</th>
            <th> Telephone</th>
            <th> Action</th>
            <th> Action</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var m in Model)
        {
            <tr>
                <td>@m.id</td>
                <td>@m.Employee</td>
                <td>@m.Work</td>
                <td>@m.Desription</td>
                <td>@m.Duration</td>
                <td>@m.Telephone</td>
                <td><a asp-action="upsert" asp-route-id="@m.id" class="btn btn-danger">Upsert </a></td>
                <td><a asp-action="delete" asp-route-id="@m.id">Delete</a></td>
            </tr>
        }
    </tbody>
</table>


